{
  "name": "veetmanager",
  "productName": "VEET Manager",
  "version": "2.2.5",
  "authors": "Meta Platforms, Inc.",
  "main": "packages/main/dist/index.js",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "config": {
    "repo_path": "arvr/projects/xred/veet/veetmanager"
  },
  "scripts": {
    "preinstall": "(node -e \"process.exit(process.platform === 'win32' ? 0 : 1)\") && ((edenfsctl info . && eden redirect add $npm_package_config_repo_path/node_modules symlink) || echo no eden found) || ((edenfsctl info . && eden redirect add $npm_package_config_repo_path/node_modules bind) || echo no eden found)",
    "build": "node scripts/build.js",
    "viteBuild": "yarn viteBuild:main && yarn viteBuild:preload && yarn viteBuild:renderer",
    "viteBuild:main": "vite build ./packages/main",
    "viteBuild:preload": "vite build ./packages/preload",
    "viteBuild:renderer": "vite build ./packages/renderer",
    "buildUnpacked": "cross-env MODE=production yarn build && electron-builder build --dir --config.asar=false",
    "buildInstallerLocal": "cross-env MODE=production yarn build && electron-builder build",
    "buildInstallers": "tsx .ci/buildInstallers.ts ./package.json",
    "checkBuildReady": "node scripts/check-build-ready.js",
    "ensureBuildReady": "node scripts/ensure-build-ready.js",
    "genAttribution": "tsx scripts/genAttribution.ts ./package.json ./ATTRIBUTION.md && yarn run marked -i ./ATTRIBUTION.md -o ./packages/renderer/public/ATTRIBUTION.html",
    "genChangelog": "yarn run marked -i ./CHANGELOG.md -o ./packages/renderer/public/CHANGELOG.html",
    "ingestCalibrationFiles": "tsx scripts/ingestCalibrationFiles ./",
    "updateFirmware": "tsx scripts/updateFirmwareManifest ./",
    "updateElectronVersion": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.js",
    "test": "yarn ensureBuildReady && yarn viteBuild && vitest run",
    "test:e2e": "yarn ensureBuildReady && yarn viteBuild && vitest run --config vitest.e2e.config.js",
    "test:main": "yarn viteBuild:main && vitest run -r packages/main --passWithNoTests",
    "test:preload": "yarn viteBuild:preload && vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "yarn viteBuild:renderer && vitest run -r packages/renderer --passWithNoTests",
    "watch": "yarn ensureBuildReady && node scripts/watch.js",
    "watch:mock": "cross-env VEET_MOCK_HARDWARE=true yarn watch",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "yarn typecheck:main && yarn typecheck:preload && yarn typecheck:renderer"
  },
  "devDependencies": {
    "@types/node": "20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cross-env": "7.0.3",
    "electron": "31.2.0",
    "electron-builder": "24.13.3",
    "eslint": "8.57.0",
    "happy-dom": "^15.10.1",
    "marked": "^13.0.2",
    "nano-staged": "0.8.0",
    "playwright": "1.45.1",
    "simple-git-hooks": "2.11.1",
    "tsx": "^4.19.3",
    "typescript": "5.5.3",
    "unplugin-auto-expose": "0.3.0",
    "vite": "^5.4.12",
    "vitest": "2.1.9"
  },
  "dependencies": {
    "@babylonjs/core": "^7.15.0",
    "async-await-queue": "^2.1.4",
    "color": "^4.2.3",
    "diskusage": "^1.2.0",
    "drivelist": "^12.0.2",
    "electron-updater": "6.3.0",
    "fs-extra": "^11.2.0",
    "json5": "^2.2.3",
    "md5-file": "^5.0.0",
    "quark-styles": "^2.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-select": "^5.8.0",
    "react-timezone-select": "^3.2.7",
    "react-tooltip": "^5.27.1",
    "recharts": "^2.12.7",
    "serialport": "^12.0.0",
    "simply-immutable": "^2.1.1",
    "ts-debounce": "^4.0.0",
    "winston": "^3.13.1",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8"
  },
  "packageManager": "yarn@1.18.0",
  "resolutions": {
    "cross-spawn": "^7.0.5"
  }
}
