{
  "name": "veetmanager",
  "productName": "veetmanager",
  "version": "2.0.2",
  "authors": "Meta Platforms, Inc.",
  "main": "packages/main/dist/index.js",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "scripts": {
    "preinstall": "((edenfsctl info . | findstr eden-backing-repos) && eden redirect add arvr/projects/xred/veet/veetmanager/node_modules symlink ) || echo no eden found",
    "build": "yarn run build:main && yarn run build:preload && yarn run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "compileUnpacked": "cross-env MODE=production yarn run build && electron-builder build --config electron-builder.json --dir --config.asar=false",
    "compileInstaller": "cross-env MODE=production yarn run build && electron-builder build --config electron-builder.json",
    "genAttribution": "tsx scripts/genAttribution.ts ./package.json ./ATTRIBUTION.md && yarn run marked -i ./ATTRIBUTION.md -o ./packages/renderer/public/ATTRIBUTION.html",
    "updateFirmware": "tsx scripts/updateFirmwareManifest ./",
    "test": "yarn run test:main && yarn run test:preload && yarn run test:renderer && yarn run test:e2e",
    "test:e2e": "yarn run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,mjs,cjs,ts,mts,cts",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:renderer": "tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck": "yarn run typecheck:main && yarn run typecheck:preload && yarn run typecheck:renderer",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.js && yarn run genAttribution"
  },
  "devDependencies": {
    "@types/node": "20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "@vitejs/plugin-react": "^4.3.1",
    "cross-env": "7.0.3",
    "electron": "31.2.0",
    "electron-builder": "24.13.3",
    "eslint": "8.57.0",
    "happy-dom": "14.12.3",
    "marked": "^13.0.2",
    "nano-staged": "0.8.0",
    "playwright": "1.45.1",
    "simple-git-hooks": "2.11.1",
    "tsx": "^4.16.2",
    "typescript": "5.5.3",
    "unplugin-auto-expose": "0.3.0",
    "vite": "5.3.3",
    "vitest": "2.0.2"
  },
  "dependencies": {
    "@babylonjs/core": "^7.15.0",
    "async-await-queue": "^2.1.4",
    "color": "^4.2.3",
    "diskusage": "^1.2.0",
    "drivelist": "^12.0.2",
    "electron-updater": "6.3.0",
    "fs-extra": "^11.2.0",
    "json5": "^2.2.3",
    "md5-file": "^5.0.0",
    "quark-styles": "^2.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-select": "^5.8.0",
    "react-timezone-select": "^3.2.5",
    "react-tooltip": "^5.27.1",
    "recharts": "^2.12.7",
    "serialport": "^12.0.0",
    "simply-immutable": "^2.1.1",
    "ts-debounce": "^4.0.0",
    "zod": "^3.23.8"
  },
  "packageManager": "yarn@4.1.1"
}
